{
  "testEnvironment": "node",
  "collectCoverageFrom": [
    "**/*.{js,jsx}",
    "!**/node_modules/**",
    "!**/coverage/**"
  ],
  "coverageReporters": [
    "text",
    "lcov",
    "html"
  ],
  "testMatch": [
    "**/__tests__/**/*.js",
    "**/?(*.)+(spec|test).js"
  ],
  "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
}
